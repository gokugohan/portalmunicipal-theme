h1, h2, h3, h4, h5, h6 {
    font-family: "Raleway", sans-serif;
}

/*--------------------------------------------------------------
# Sections General
--------------------------------------------------------------*/
.container{
    max-width: 85% !important;
}

section {
    padding: 60px 0;
    overflow: hidden;
}

.section-bg {
    background-color: #f6f7f6;
}

.section-title {
    padding-bottom: 40px;
}
.section-title h2 {
    font-size: 14px;
    font-weight: 500;
    padding: 0;
    line-height: 1px;
    margin: 0 0 5px 0;
    letter-spacing: 2px;
    text-transform: uppercase;
    color: #aaaaaa;
    font-family: "Poppins", sans-serif;
}
.section-title h2::after {
    content: "";
    width: 120px;
    height: 1px;
    display: inline-block;
    background: #9ae1af;
    margin: 4px 10px;
}
.section-title p {
    margin: 0;
    margin: 0;
    font-size: 36px;
    font-weight: 700;
    text-transform: uppercase;
    font-family: "Poppins", sans-serif;
    color: #37423b;
}

.breadcrumbs {
    margin-top: 73px;
    text-align: center;
    background: #5fcf80;
    padding: 30px 0;
    color: #fff;
}
@media (max-width: 992px) {
    .breadcrumbs {
        margin-top: 63px;
    }
}
.breadcrumbs h2 {
    font-size: 32px;
    font-weight: 500;
}
.breadcrumbs p {
    font-size: 14px;
    margin-bottom: 0;
}

.breadcrumb-container {
    background: #960807;
}

.learn-press-breadcrumb {
    width: 85% !important;
    margin: 0 auto !important;
    text-align: left;
    font-size: 16px !important;
    padding: 30px 15px !important;
    color: #ddd !important;
    font-weight: 600 !important;
    /*border-bottom: 1px solid #fff;*/
}

.learn-press-breadcrumb li a {
    color: #fff !important;
    font-weight: 600 !important;
}

.lp-courses-bar .search-courses button {
    margin: 0 !important;
}




ul.learn-press-nav-tabs .course-nav.active::before,
.lp-archive-courses .course-summary .course-summary-content .course-detail-info {
    background: #960807 !important;
}

.course-faqs-box .course-faqs-box__title:hover, input[name=course-faqs-box-ratio]:checked + .course-faqs-box .course-faqs-box__title {
    color: #960807 !important;
}




.lp-archive-courses .course-readmore a,
#learn-press-course button.button-enroll-course {
    background: #960807 !important;
    border-color: #960807;
}

.lp-content-area {
    margin-top: 20px !important;
    width: 85% !important;
}

#learn-press-profile .lp-content-area{
    width:1170px !important ;
}

.lp-content-area .section-title{
    background: none;
}
.learn-press-progress::before {
    /*background: #960807 !important;*/
}

#learn-press-course-curriculum.course-curriculum ul.curriculum-sections {
    z-index: inherit !important;
}

.learn-press-message.success {
    border-top: 5px solid #960807 !important;
}

.learn-press-message.success::before, #popup-course #popup-header {
    background: #960807 !important;
}

#popup-course .back-course i.fa.fa-times {
    transform: translateY(25px);
}

#learn-press-course .course-summary-sidebar .course-sidebar-preview .lp-course-buttons button {
    margin: 0;
    background: #960807 !important;
    color: #fff;
}

ul.learn-press-nav-tabs .course-nav {
    background: #960807;
    color: #fff;
}


ul.learn-press-nav-tabs .course-nav:hover label {
    color: #fff9c0 !important;
}

/*ul.learn-press-nav-tabs .course-nav.active label, */
.comment-form #submit.section-item-link::before {
    color: #960807 !important;
}

ul.learn-press-nav-tabs .course-nav.active::before {
    height: 2px !important;
}

#learn-press-profile #profile-nav .lp-profile-nav-tabs>li>a>i{
    color: #960800f7 !important;
}

.learn-press-tabs .learn-press-tabs__tab:before,
.learn-press-tabs .learn-press-tabs__tab:hover{
    background: #960800f7 !important;
}

.learn-press-tabs .learn-press-tabs__tab:hover a{
    color: #fff !important;
}
.learn-press-tabs .learn-press-tabs__tab label a, .learn-press-profile-course__tab a, .learn-press-filters>li span{
    color: #960800f7 !important;
}
.learn-press-profile-course__tab a.active{
    border-top: 4px solid #960800f7 !important;
}
.lp-profile-content button{
    margin: 0;
    padding: 16px 24px;
    border: 0;
    border-radius: 3px;
    color: #fff;
    background: #960800f7 !important;
    font-size: 16px;
    font-weight: 400;
    line-height: 1;
}

.lp-profile-content button:hover{
    background: #960807 !important;
}

.learn-press-progress .progress-bg .progress-active,
.lp-profile-content button,
#learn-press-profile #profile-nav .lp-profile-nav-tabs li.active,
#learn-press-profile #profile-nav .lp-profile-nav-tabs li:hover{
    background: #960800f7 !important;
}


.learnpress_avatar>div>div{
    margin-top: 10px;
}
.learnpress_avatar>div>div>button{
    margin-left: 5px;
}


#learn-press-course-tabs .course-nav.active label,
.lp-courses-bar .switch-layout .switch-btn.list::before,
.lp-courses-bar .switch-layout .switch-btn.grid::before,
.lp-archive-courses .course .course-item .course-content .course-permalink .course-title:hover{
    color: #960807 !important;
}

.lp-archive-courses .course .course-item .course-content .course-readmore a:hover{
    opacity: 0.8 !important;
}

/*--------------------------------------------------------------
# Hero Section
--------------------------------------------------------------*/
#platform-education-hero {
    width: 100%;
    height: 80vh;
    /*background: url("../img/pages.jpg") top center;*/
    background-size: cover;
	background-attachment:fixed;
    position: relative;
}
#platform-education-hero:before {
    content: "";
    background: rgba(0, 0, 0, 0.4);
    position: absolute;
    bottom: 0;
    top: 0;
    left: 0;
    right: 0;
}
#platform-education-hero .container {
    padding-top: 72px;
}
@media (max-width: 992px) {
    #platform-education-hero .container {
        padding-top: 62px;
    }
}
#platform-education-hero h1 {
    margin: 0;
    font-size: 48px;
    font-weight: 700;
    line-height: 56px;
    color: #fff;
    font-family: "Poppins", sans-serif;
}
#platform-education-hero h2 {
    color: #eee;
    margin: 10px 0 0 0;
    font-size: 24px;
}
#platform-education-hero .btn-get-started {
    font-family: "Raleway", sans-serif;
    font-weight: 500;
    font-size: 15px;
    letter-spacing: 1px;
    display: inline-block;
    padding: 10px 35px;
    border-radius: 50px;
    transition: 0.5s;
    margin-top: 30px;
    border: 2px solid #960807;
    color: #fff;
}
#platform-education-hero .btn-get-started:hover {
    background: #960807;
    border: 2px solid #960807;
}
@media (min-width: 1024px) {
    #platform-education-hero {
        background-attachment: fixed;
    }
}
@media (max-width: 768px) {
    #platform-education-hero {
        height: 100vh;
    }
    #platform-education-hero h1 {
        font-size: 28px;
        line-height: 36px;
    }
    #platform-education-hero h2 {
        font-size: 18px;
        line-height: 24px;
    }
}


/*--------------------------------------------------------------
# About
--------------------------------------------------------------*/
.about .content h3 {
    font-weight: 600;
    font-size: 26px;
}
.about .content ul {
    list-style: none;
    padding: 0;
}
.about .content ul li {
    padding-bottom: 10px;
}
.about .content ul i {
    font-size: 20px;
    padding-right: 4px;
    color: #5fcf80;
}
.about .content .learn-more-btn {
    background: #5fcf80;
    color: #fff;
    border-radius: 50px;
    padding: 8px 25px 9px 25px;
    white-space: nowrap;
    transition: 0.3s;
    font-size: 14px;
    display: inline-block;
}
.about .content .learn-more-btn:hover {
    background: #3ac162;
    color: #fff;
}
@media (max-width: 768px) {
    .about .content .learn-more-btn {
        margin: 0 48px 0 0;
        padding: 6px 18px;
    }
}

/*--------------------------------------------------------------
# Why Us
--------------------------------------------------------------*/
.why-us .content {
    padding: 30px;
    background: #960807;
    border-radius: 4px;
    color: #fff;
}
.why-us .content h3 {
    font-weight: 700;
    font-size: 34px;
    margin-bottom: 30px;
    text-shadow: none;
    color: #fff;
}
.why-us .content p {
    margin-bottom: 30px;
}
.why-us .content .more-btn {
    display: inline-block;
    background: rgba(255, 255, 255, 0.2);
    padding: 6px 30px 8px 30px;
    color: #fff;
    border-radius: 50px;
    transition: all ease-in-out 0.4s;
}
.why-us .content .more-btn i {
    font-size: 14px;
}
.why-us .content .more-btn:hover {
    color: #960807;
    background: #fff;
}
.why-us .icon-boxes .icon-box {
    text-align: center;
    background: #fff;
    padding: 40px 30px;
    width: 100%;
    border: 1px solid #eef0ef;
}
.why-us .icon-boxes .icon-box i {
    font-size: 32px;
    padding: 18px;
    color: #960807;
    /*margin-bottom: 30px;*/
    background: #ecf9f0;
    border-radius: 50px;
}
.why-us .icon-boxes .icon-box h4 {
    font-size: 20px;
    font-weight: 700;
    margin: 30px 0 30px 0;
}
.why-us .icon-boxes .icon-box p {
    font-size: 15px;
    color: #848484;
}

/*--------------------------------------------------------------
# Trainers
--------------------------------------------------------------*/
.trainers .member {
    text-align: center;
    margin-bottom: 20px;
    background: #fff;
    border: 1px solid #eef0ef;
}
.trainers .member img {
    margin: -1px -1px 30px -1px;
}
.trainers .member .member-content {
    padding: 0 20px 30px 20px;
}
.trainers .member h4 {
    font-weight: 700;
    margin-bottom: 2px;
    font-size: 18px;
}
.trainers .member span {
    font-style: italic;
    display: block;
    font-size: 13px;
}
.trainers .member p {
    padding-top: 10px;
    font-size: 14px;
    font-style: italic;
    color: #aaaaaa;
}
.trainers .member .social {
    margin-top: 15px;
}
.trainers .member .social a {
    color: #7e9486;
    transition: 0.3s;
}
.trainers .member .social a:hover {
    color: #960807;
}
.trainers .member .social i {
    font-size: 18px;
    margin: 0 2px;
}
