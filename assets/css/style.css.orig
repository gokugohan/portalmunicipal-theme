stroke-width: 0.7px;
cursor: pointer;
}
text tspan {
font-size: 9px ;
fill: #0000008a;
stroke: unset;
font-weight: 300;

}

span.library {
  padding-left: 50px;
  position: absolute;
  top: 12px;
  z-index: 1;
}

 
.top_h_1{
     
  justify-content: left;
  border: solid #d4dae0;
  width: 100%;
  -webkit-box-shadow: 0 3px 4px 0 rgba(50,50,50,0.15);
  -moz-box-shadow: 0 3px 4px 0 rgba(50,50,50,0.15);
  box-shadow: 0 3px 4px 0 rgba(50,50,50,0.15);
  background: #f6f9fb;
  padding: 5px 0;
  border-width: 1px 0;
  height:50px;
  margin-top:0px !important;
  position: sticky;
  top: 110px;
}

.partners{
  display:flex;
  width: 100%;
  list-style: none;
  margin-bottom: 0;

}

.partners li{
  width:25%;
  text-align:center;
  
}
.partners li img {
    width: 100px;
}


 

